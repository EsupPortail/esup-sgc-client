		target="$INSTALL_PATH\java\bin\javaw.exe" commandLine="--module-path javafx\lib --add-modules javafx.controls,javafx.fxml,javafx.base,javafx.media,javafx.graphics,javafx.swing,javafx.web -jar esupsgcclient-shib.jar"
